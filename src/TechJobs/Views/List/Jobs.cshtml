<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>
    
@{ 
}

<style>
    td{
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
    }
    tr:nth-child(even) {
        background-color: ghostwhite;
    }
</style>


<table>
    @foreach (Dictionary<string, string> job in ViewBag.jobs)
    {


        @foreach (KeyValuePair<string, string> singleJob in job)
        {
            <tr>
                <td>@singleJob.Key : @singleJob.Value</td>
            </tr>
        }



    }
</table>